#!/bin/sh

set -e

THIS_PACKAGE=xkb-data
THIS_SCRIPT=postinst

#INCLUDE_SHELL_LIB#

case $1 in
  configure)
  if dpkg --compare-versions "$2" lt-nl 1.6-2; then
    remove_conffile_commit /etc/X11/xkb/base.xml
    rmdir --ignore-fail-on-non-empty /etc/X11/xkb 2>/dev/null
  fi
  ;;
esac

