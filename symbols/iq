// Iraque keyboard layout,

// 3-Level layout

partial default alphanumeric_keys
xkb_symbols "basic" {
    include "ara(basic)"
    name[Group1]= "Iraq";
};

partial alphanumeric_keys
xkb_symbols "ku" {
    include "tr(ku)"
    name[Group1]= "Iraq - Kurdish, Latin Q";
};

partial alphanumeric_keys
xkb_symbols "ku_f" {
    include "tr(ku_f)"
    name[Group1]= "Iraq - Kurdish, (F)";
};

partial alphanumeric_keys
xkb_symbols "ku_alt" {
    include "tr(ku_alt)"
    name[Group1]= "Iraq - Kurdish, Latin Alt-Q";
};
